{
  "name": "jupyterlab-datawidgets",
  "version": "2.0.1",
  "description": "A JupyterLab extension providing a set of widgets to help facilitate reuse of large datasets across widgets.",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "dependencies": {
    "@jupyter-widgets/jupyterlab-manager": "~0.25.11",
    "@jupyterlab/application": "~0.9.0",
    "@phosphor/coreutils": "~1.3.0",
    "jupyter-datawidgets": "^2.0.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.2"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension",
    "jupyterlab notebook"
  ],
  "jupyterlab": {
    "extension": true
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc --project src",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vidartf/jupyter-datawidgets"
  },
  "files": [
    "build/*.js"
  ],
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vidartf/jupyter-datawidgets/issues"
  },
  "homepage": "https://github.com/vidartf/jupyter-datawidgets"
}
